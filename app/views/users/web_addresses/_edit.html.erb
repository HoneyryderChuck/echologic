<% web_address ||= @web_address %>
<% content_for :fields do %>
  <%= select :web_address, :type_id,
             WebAddress.web_address_types.collect { |wa| [wa.get_current_enum_value.value,wa.id] },
             { :selected => web_address.type_id } %>
  <%= text_field :web_address, :address %>
<% end %>
<%= render :partial => 'users/components/edit', :locals => {:type => 'web_address', :object => web_address} %>