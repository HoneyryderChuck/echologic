<div class="roadmap_tab">	
	<%= image_tag("../images/page/icons/under_construction_big.png", :width => 25, :height => 28, :border => "none") %>				
</div>
<%
# Render submenu tabs for act section.

render_tabnav :act,
              :generate_css => false, :html => {:class => 'submenu'} do

  add_tab :html => {:id => 'roadmap_tab'} do |t|    
    value = "#{Development::ACT_PROGRESS}%  " + I18n.t('application.roadmap.title')
    t.named value
    t.links_to hash_for_act_path
  end


end
%>
