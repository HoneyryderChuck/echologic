var elements_list = $("<%= escape_javascript(render :partial => 'statements/child',
                                                    :locals  => {:child_type => @type.to_s.underscore,
                                                                 :new_level => @node_environment.new_level?,
                                                                 :hub => @node_environment.hub.to_s},
                                                    :collection => @children[@type][@offset, @per_page]) %>");


var statement = $('#<%=dom_id(@statement_node)%>');
statement.data('statement').insertChildren('<%= @type.to_s.underscore.pluralize %>', [elements_list], <%= @page %> );
